{
  "name": "reynard-prompt-note",
  "version": "0.1.0",
  "description": "Prompt Note - OneNote-like note-taking application with gamification, built with Reynard framework",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "dev:backend": "cd backend && python main.py",
    "setup:db": "cd backend && python setup_database.py",
    "start:full": "concurrently \"pnpm run dev\" \"pnpm run dev:backend\""
  },
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-3d": "workspace:*",
    "reynard-algorithms": "workspace:*",
    "reynard-components-core": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*",
    "reynard-auth": "workspace:*",
    "reynard-chat": "workspace:*",
    "reynard-composables": "workspace:*",
    "reynard-monaco": "workspace:*",
    "reynard-caption": "workspace:*",
    "reynard-annotating-core": "workspace:*",
    "solid-js": "1.9.9",
    "@solidjs/router": "0.15.3",
    "date-fns": "4.1.0",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@types/uuid": "10.0.0",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "concurrently": "9.2.1"
  }
}