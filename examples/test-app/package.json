{
  "name": "reynard-test-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=test-app-example ../../scripts/testing/vitest-global-queue.sh run test-app-example",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "validate-story": "pnpm test -- --run"
  },
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-3d": "workspace:*",
    "reynard-algorithms": "workspace:*",
    "reynard-components-core": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*",
    "reynard-charts": "workspace:*",
    "reynard-colors": "workspace:*",
    "chart.js": "4.5.0",
    "chartjs-adapter-date-fns": "3.0.0",
    "date-fns": "4.1.0",
    "solid-js": "1.9.9",
    "three": "0.180.0"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@types/three": "0.180.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "18.0.1",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
