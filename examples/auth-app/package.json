{
  "name": "reynard-auth-app",
  "version": "2.0.0",
  "type": "module",
  "description": "A comprehensive authentication demo app with RBAC integration built with Reynard framework and Gatekeeper backend",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "backend": "python backend/main.py",
    "dev:full": "concurrently \"pnpm run backend\" \"pnpm run dev\""
  },
  "dependencies": {
    "reynard-auth": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-components-core": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*",
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "concurrently": "^8.2.2",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8"
  },
  "keywords": ["reynard", "solidjs", "authentication", "gatekeeper", "postgresql", "jwt", "rbac", "role-based-access-control"],
  "author": "Reynard Framework",
  "license": "MIT"
}
