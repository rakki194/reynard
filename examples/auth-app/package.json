{
  "name": "reynard-auth-app",
  "version": "1.0.0",
  "type": "module",
  "description": "A comprehensive authentication demo app built with Reynard framework and Gatekeeper backend",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "backend": "python backend/main.py",
    "dev:full": "concurrently \"pnpm run backend\" \"pnpm run dev\""
  },
  "dependencies": {
    "reynard-auth": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-components": "workspace:*",
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "concurrently": "^8.2.2",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8"
  },
  "keywords": [
    "reynard",
    "solidjs",
    "authentication",
    "gatekeeper",
    "postgresql",
    "jwt"
  ],
  "author": "Reynard Framework",
  "license": "MIT"
}
