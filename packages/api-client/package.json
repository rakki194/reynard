{
  "name": "reynard-api-client",
  "version": "0.1.0",
  "description": "Auto-generated TypeScript API client for Reynard backend",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./generated": {
      "types": "./dist/generated/index.d.ts",
      "import": "./dist/generated/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "generate": "openapi-generator-cli generate -i http://localhost:8000/openapi.json -g typescript-fetch -o ./src/generated --additional-properties=typescriptThreePlus=true,supportsES6=true,withInterfaces=true,useSingleRequestParameter=true",
    "generate:watch": "nodemon --watch ../../backend --exec 'pnpm run generate'",
    "build": "tsc && cp -r src/generated/* dist/generated/",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist src/generated",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "reynard-i18n": "workspace:*",
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.23.1",
    "@types/node": "24.4.0",
    "nodemon": "3.1.10",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
