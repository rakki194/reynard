{
  "name": "reynard-annotating-jtp2",
  "version": "0.2.0",
  "description": "JTP2 (Joint Tagger Project PILOT2) caption generator for Reynard - specialized for furry artwork tagging",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "annotation",
    "caption",
    "tagging",
    "jtp2",
    "furry",
    "ai",
    "ml"
  ],
  "dependencies": {
    "reynard-annotating-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
