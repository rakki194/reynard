{
  "name": "reynard-nlweb",
  "version": "0.1.0",
  "description": "NLWeb assistant tooling and routing system for Reynard",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./service": {
      "import": "./dist/service/index.js",
      "types": "./dist/service/index.d.ts"
    },
    "./router": {
      "import": "./dist/router/index.js",
      "types": "./dist/router/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=nlweb-package ../../scripts/vitest-global-queue.sh run nlweb-package",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "reynard-core": "0.1.2",
    "reynard-tools": "0.1.0",
    "reynard-service-manager": "0.1.0",
    "reynard-connection": "0.1.0"
  },
  "devDependencies": {
    "@types/node": "24.4.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "eslint": "9.35.0"
  },
  "keywords": ["nlweb", "assistant", "tooling", "routing", "ai", "reynard"],
  "author": "Balazs Horvath",
  "license": "MIT"
}
