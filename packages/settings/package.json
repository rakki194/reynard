{
  "name": "reynard-settings",
  "version": "0.2.0",
  "description": "Reynard Settings - Comprehensive settings and preferences management for SolidJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist/**/*", "README.md"],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=settings-package ../../scripts/vitest-global-queue.sh run settings-package",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "solidjs",
    "settings",
    "preferences",
    "configuration",
    "localstorage",
    "state-management",
    "theming",
    "user-preferences"
  ],
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-components": "workspace:*",
    "reynard-composables": "workspace:*",
    "reynard-connection": "workspace:*"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
