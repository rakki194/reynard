{
  "name": "reynard-settings",
  "version": "0.1.2",
  "description": "Reynard Settings - Comprehensive settings and preferences management for SolidJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "solidjs",
    "settings",
    "preferences",
    "configuration",
    "localstorage",
    "state-management",
    "theming",
    "user-preferences"
  ],
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-components": "workspace:*",
    "reynard-composables": "workspace:*"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@testing-library/jest-dom": "6.8.0",
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "jsdom": "26.1.0",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
