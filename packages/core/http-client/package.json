{
  "name": "reynard-http-client",
  "version": "0.17.1",
  "type": "module",
  "description": "HTTP client system for Reynard framework with middleware support, retry logic, and comprehensive error handling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../../vitest.env.global && VITEST_AGENT_ID=http-client-package ../../../scripts/testing/vitest-global-queue.sh run http-client-package",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["http", "client", "middleware", "retry", "circuit-breaker", "reynard", "typescript"],
  "author": "Balazs Horvath",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/rakki194/reynard"
  }
}
