{
  "name": "reynard-collaboration",
  "version": "0.1.0",
  "description": "Real-time collaboration system for Reynard applications with Yjs, Monaco Editor, and WebRTC integration",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../../vitest.env.global && VITEST_AGENT_ID=collaboration-package ../../../scripts/testing/vitest-global-queue.sh run collaboration-package",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "test:docs": "vitest run --config vitest.docs.config.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["collaboration", "realtime", "yjs", "monaco", "webrtc", "reynard", "websocket"],
  "author": "Balazs Horvath",
  "license": "MIT",
  "dependencies": {
    "yjs": "13.6.27",
    "y-monaco": "0.1.6",
    "y-webrtc": "10.3.0",
    "y-indexeddb": "9.0.12",
    "y-websocket": "3.0.0",
    "y-protocols": "1.0.6",
    "reynard-connection": "workspace:*",
    "reynard-monaco": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-auth": "workspace:*",
    "reynard-validation": "workspace:*",
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "18.0.1",
    "reynard-testing": "workspace:*",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
