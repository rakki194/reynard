{
  "name": "reynard-audio",
  "version": "0.2.0",
  "description": "Audio processing and playback components for Reynard applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=audio-package ../../scripts/vitest-global-queue.sh run audio-package",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "reynard",
    "audio",
    "media",
    "processing",
    "playback",
    "waveform",
    "analysis"
  ],
  "author": "Reynard Framework",
  "license": "MIT",
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-components": "workspace:*",
    "reynard-file-processing": "workspace:*",
    "reynard-themes": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
