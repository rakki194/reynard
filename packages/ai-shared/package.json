{
  "name": "reynard-ai-shared",
  "version": "0.1.3",
  "description": "Shared AI/ML utilities and base classes for Reynard - provides common interfaces, types, and utilities for all AI/ML packages",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.js",
      "require": "./dist/models/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=ai-shared-package ../../scripts/testing/vitest-global-queue.sh run ai-shared-package",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["ai", "ml", "shared", "utilities", "base-classes", "types", "reynard"],
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-connection": "workspace:*",
    "reynard-validation": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "reynard-testing": "workspace:*",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
