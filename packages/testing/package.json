{
  "name": "reynard-testing",
  "version": "0.1.2",
  "description": "Unified testing framework for Reynard packages",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./mocks": {
      "import": "./dist/mocks/index.js",
      "types": "./dist/mocks/index.d.ts"
    },
    "./fixtures": {
      "import": "./dist/fixtures/index.js",
      "types": "./dist/fixtures/index.d.ts"
    },
    "./doc-tests": {
      "import": "./dist/doc-tests/index.js",
      "types": "./dist/doc-tests/index.d.ts"
    },
    "./doc-test-runner": {
      "import": "./dist/doc-test-runner/index.js",
      "types": "./dist/doc-test-runner/index.d.ts"
    }
  },
  "files": [
    "dist",
    "configs"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc --build",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "test:docs": "vitest run --config vitest.docs.config.ts",
    "doc-tests": "node src/doc-test-runner.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "testing",
    "vitest",
    "solidjs",
    "reynard",
    "test-utils",
    "mocks",
    "fixtures"
  ],
  "author": "Reynard Team",
  "license": "MIT",
  "dependencies": {
    "solid-js": "1.9.9",
    "@solidjs/testing-library": "0.8.10",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/user-event": "14.6.1"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "typescript": "5.9.2",
    "vite": "5.4.20",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
