{
  "name": "reynard-segmentation",
  "version": "0.1.0",
  "description": "Polygon segmentation system for Reynard annotation framework",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "source ../../vitest.env.global && ../../scripts/vitest-global-queue.sh run segmentation",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.config.ts src/__tests__/integration.test.ts",
    "test:performance": "vitest run --config vitest.config.ts src/__tests__/performance.test.ts",
    "test:unit": "vitest run --config vitest.config.ts src/__tests__/*.test.ts --exclude src/__tests__/integration.test.ts src/__tests__/performance.test.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": ["reynard", "segmentation", "polygon", "annotation", "computer-vision", "image-processing"],
  "author": "Reynard Team",
  "license": "MIT",
  "dependencies": {
    "reynard-ai-shared": "workspace:*",
    "reynard-algorithms": "workspace:*",
    "reynard-annotating-core": "workspace:*",
    "reynard-floating-panel": "workspace:*",
    "reynard-caption-core": "workspace:*",
    "reynard-caption-ui": "workspace:*",
    "reynard-boundingbox": "workspace:*",
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "18.0.1",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4",
    "eslint": "9.35.0"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
