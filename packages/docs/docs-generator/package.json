{
  "name": "reynard-docs-generator",
  "version": "0.1.2",
  "description": "Automated documentation generator for Reynard packages",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "bin": {
    "reynard-docs": "./bin/reynard-docs"
  },
  "files": ["dist", "bin", "loader.mjs"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "test": "source ../../../vitest.env.global && VITEST_AGENT_ID=docs-generator-package ../../../scripts/testing/vitest-global-queue.sh run docs-generator-package",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "generate": "node --loader ./loader.mjs dist/cli/index.js",
    "generate:watch": "node --loader ./loader.mjs dist/cli/index.js --watch"
  },
  "keywords": ["documentation", "generator", "automation", "reynard", "typescript"],
  "author": "Balazs Horvath",
  "license": "MIT",
  "dependencies": {
    "chokidar": "4.0.3",
    "extensionless": "^1.9.9",
    "fs-extra": "11.3.1",
    "glob": "11.0.3",
    "gray-matter": "4.0.3",
    "highlight.js": "11.11.1",
    "marked": "16.2.1",
    "reynard-docs-core": "workspace:*",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
