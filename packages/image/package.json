{
  "name": "reynard-image",
  "version": "0.1.0",
  "description": "Image processing and display components for Reynard applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "reynard",
    "image",
    "media",
    "processing",
    "display",
    "gallery",
    "metadata"
  ],
  "author": "Reynard Framework",
  "license": "MIT",
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-components": "workspace:*",
    "reynard-file-processing": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "solid-js": "^1.8.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-solid": "^2.8.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "solid-js": "^1.8.0"
  }
}
