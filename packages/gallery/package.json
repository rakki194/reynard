{
  "name": "reynard-gallery",
  "version": "0.2.0",
  "description": "Reynard Gallery - File and media management components for SolidJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist/**/*", "README.md"],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=gallery-package ../../scripts/testing/vitest-global-queue.sh run gallery-package",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "solidjs",
    "gallery",
    "file-management",
    "media-viewer",
    "responsive-grid",
    "image-gallery",
    "file-upload"
  ],
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-components-core": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*",
    "reynard-composables": "workspace:*",
    "reynard-i18n": "workspace:*",
    "@solid-primitives/intersection-observer": "2.2.2",
    "@solid-primitives/resize-observer": "2.1.3",
    "@solid-primitives/event-listener": "2.4.3"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
