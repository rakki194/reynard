{
    "name": "reynard-humility-parser",
    "version": "1.0.0",
    "description": "TypeScript parser for humility detector output reports",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "humility",
        "parser",
        "analysis",
        "reynard",
        "typescript"
    ],
    "author": "Reynard Team",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "24.5.2",
        "@typescript-eslint/eslint-plugin": "8.44.0",
        "@typescript-eslint/parser": "8.44.0",
        "eslint": "9.36.0",
        "typescript": "5.9.2",
        "vitest": "3.2.4"
    },
    "dependencies": {
        "zod": "4.1.9"
    }
}