{
  "name": "reynard-annotating-florence2",
  "version": "0.2.0",
  "description": "Florence2 caption generator for Reynard - general purpose image captioning with multiple tasks",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=annotating-florence2-package ../../scripts/vitest-global-queue.sh run annotating-florence2-package",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "annotation",
    "caption",
    "florence2",
    "general_purpose",
    "ai",
    "ml"
  ],
  "dependencies": {
    "reynard-annotating-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
