{
  "name": "reynard-animation",
  "version": "0.1.0",
  "description": "ðŸ¦Š Unified animation system for Reynard - consolidates all animation functionality",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./easing": {
      "types": "./dist/easing/index.d.ts",
      "import": "./dist/easing/index.js"
    },
    "./engines": {
      "types": "./dist/engines/index.d.ts",
      "import": "./dist/engines/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@types/node": "24.4.0",
    "eslint": "9.35.0",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "keywords": ["animation", "solidjs", "reynard", "easing", "performance", "3d", "staggered"],
  "author": "Reynard Team",
  "license": "MIT"
}
