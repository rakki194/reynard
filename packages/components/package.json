{
  "name": "@reynard/components",
  "version": "0.1.0",
  "description": "Reynard UI Components - Production-ready SolidJS component library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc -p tsconfig.json && mv dist/components.css dist/styles.css",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solidjs",
    "ui",
    "components",
    "design-system",
    "accessibility",
    "responsive"
  ],
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "@reynard/core": "file:../core"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@testing-library/jest-dom": "6.8.0",
    "@types/node": "24.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "jsdom": "26.1.0",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.4",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
