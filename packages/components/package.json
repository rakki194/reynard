{
  "name": "reynard-components",
  "version": "0.3.1",
  "description": "Reynard UI Components - Production-ready SolidJS component library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.js",
      "require": "./dist/icons/index.cjs"
    },
    "./styles": "./dist/styles.css",
    "./dist/styles.css": "./dist/styles.css"
  },
  "files": ["dist/**/*", "README.md"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly && (mv dist/components.css dist/styles.css 2>/dev/null || mv dist/asset dist/styles.css 2>/dev/null || true)",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=components-package ../../scripts/vitest-global-queue.sh run components-package",
    "test:coverage": "vitest --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "test:docs": "vitest run --config vitest.docs.config.ts",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["solidjs", "ui", "components", "design-system", "accessibility", "responsive"],
  "peerDependencies": {
    "solid-js": "1.9.9",
    "three": "^0.180.0"
  },
  "peerDependenciesMeta": {
    "solid-js": {
      "optional": false
    },
    "three": {
      "optional": true
    }
  },
  "dependencies": {
    "reynard-charts": "workspace:*",
    "reynard-colors": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-i18n": "workspace:*",
    "reynard-themes": "workspace:*"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@types/three": "0.180.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
