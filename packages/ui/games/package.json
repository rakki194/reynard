{
  "name": "reynard-games",
  "version": "0.2.0",
  "description": "Interactive games and visualizations for Reynard framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./union-find": {
      "types": "./dist/union-find/index.d.ts",
      "import": "./dist/union-find/index.js"
    },
    "./collision": {
      "types": "./dist/collision/index.d.ts",
      "import": "./dist/collision/index.js"
    },
    "./spatial-hash": {
      "types": "./dist/spatial-hash/index.d.ts",
      "import": "./dist/spatial-hash/index.js"
    },
    "./geometry": {
      "types": "./dist/geometry/index.d.ts",
      "import": "./dist/geometry/index.js"
    },
    "./performance": {
      "types": "./dist/performance/index.d.ts",
      "import": "./dist/performance/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc src/index.ts --declaration --declarationMap --emitDeclarationOnly --outDir dist --jsx preserve --jsxImportSource solid-js --target ES2022 --moduleResolution bundler --skipLibCheck",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../../vitest.env.global && VITEST_AGENT_ID=games-package ../../../scripts/testing/vitest-global-queue.sh run games-package",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "solid-js": "1.9.9",
    "reynard-primitives": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-i18n": "workspace:*",
    "reynard-themes": "workspace:*"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "18.0.1",
    "reynard-testing": "0.1.2",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
