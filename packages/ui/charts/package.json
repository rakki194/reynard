{
  "name": "reynard-charts",
  "version": "0.2.0",
  "description": "Reynard Charts - Data visualization components for SolidJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "test": "source ../../../vitest.env.global && VITEST_AGENT_ID=charts-package ../../../scripts/testing/vitest-global-queue.sh run charts-package",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "solidjs",
    "charts",
    "data-visualization",
    "chartjs",
    "graphs",
    "analytics"
  ],
  "peerDependencies": {
    "chart.js": "4.5.0",
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-colors": "workspace:*",
    "reynard-themes": "workspace:*",
    "chartjs-adapter-date-fns": "3.0.0",
    "date-fns": "4.1.0",
    "three": "0.180.0"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@types/three": "0.180.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "chart.js": "4.5.0",
    "happy-dom": "18.0.1",
    "reynard-testing": "0.1.2",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}