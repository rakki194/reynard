{
  "name": "reynard-components-core",
  "version": "0.1.0",
  "description": "Core UI primitives, navigation, and layout components for Reynard",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "import": "./dist/primitives/index.js"
    },
    "./navigation": {
      "types": "./dist/navigation/index.d.ts",
      "import": "./dist/navigation/index.js"
    },
    "./layout": {
      "types": "./dist/layout/index.d.ts",
      "import": "./dist/layout/index.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "pnpm clean && vite build && tsc -p tsconfig.build.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:watch": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "pnpm clean:all",
    "clean:all": "rm -rf dist && rm -f *.tsbuildinfo && find . -name '*.js' -not -path './node_modules/*' -not -path './dist/*' -delete && find . -name '*.d.ts' -not -path './node_modules/*' -not -path './dist/*' -delete && find . -name '*.js.map' -not -path './node_modules/*' -not -path './dist/*' -delete && find . -name '*.d.ts.map' -not -path './node_modules/*' -not -path './dist/*' -delete && find . -name '*.jsx' -not -path './node_modules/*' -not -path './dist/*' -delete",
    "clean:build": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "prebuild": "pnpm clean:build"
  },
  "keywords": ["components", "primitives", "navigation", "layout", "core"],
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-themes": "workspace:*"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
