{
  "name": "@reynard/fluent-icons",
  "version": "0.1.0",
  "description": "Fluent UI icons for Reynard design system",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "reynard",
    "icons",
    "fluent-ui",
    "design-system"
  ],
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "@fluentui/svg-icons": "1.1.309",
    "@reynard/core": "file:../core"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^26.1.0",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.4",
    "vite-plugin-solid": "2.11.8",
    "vitest": "^2.1.8"
  }
}
