{
  "name": "reynard-fluent-icons",
  "version": "0.1.0",
  "description": "Fluent UI icons for Reynard design system",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "npm run clean && vite build && npm run build:declarations",
    "build:declarations": "node scripts/build-declarations.js",
    "generate-types": "node scripts/generate-types.js",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fluentui/svg-icons": "1.1.309",
    "reynard-core": "file:../core",
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "typescript": "5.9.2",
    "vite": "7.1.4",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
