{
  "name": "reynard-gallery-dl",
  "version": "0.1.0",
  "description": "Gallery-dl integration for Reynard - powerful media download system with progress tracking and batch processing",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc -p tsconfig.json --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../../vitest.env.global && VITEST_AGENT_ID=gallery-dl-package ../../../scripts/testing/vitest-global-queue.sh run gallery-dl-package",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["gallery-dl", "media", "download", "batch", "progress", "extraction", "reynard"],
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-ai-shared": "workspace:*",
    "reynard-service-manager": "workspace:*",
    "reynard-connection": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-floating-panel": "workspace:*",
    "reynard-components-core": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "18.0.1",
    "reynard-testing": "workspace:*",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
