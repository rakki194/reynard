{
  "name": "reynard-rag",
  "version": "0.2.0",
  "description": "Reynard RAG System - Retrieval-Augmented Generation components for SolidJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/styles.css",
    "./dist/styles.css": "./dist/styles.css"
  },
  "files": ["dist/**/*", "README.md"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly && (mv dist/rag.css dist/styles.css 2>/dev/null || mv dist/asset dist/styles.css 2>/dev/null || true)",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../../vitest.env.global && VITEST_AGENT_ID=rag-package ../../../scripts/testing/vitest-global-queue.sh run rag-package",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["solidjs", "rag", "retrieval", "search", "embeddings", "ai", "ui"],
  "dependencies": {
    "solid-js": "1.9.9",
    "reynard-api-client": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-primitives": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*",
    "reynard-themes": "workspace:*"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
