{
  "name": "reynard-annotating-ui",
  "version": "0.1.0",
  "description": "UI components for Reynard annotation system - batch processing and caption generation components",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../../vitest.env.global && VITEST_AGENT_ID=annotating-ui-package ../../../scripts/testing/vitest-global-queue.sh run annotating-ui-package",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["annotation", "ui", "components", "batch", "caption", "solidjs"],
  "dependencies": {
    "reynard-annotating-core": "workspace:*",
    "reynard-primitives": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "reynard-testing": "workspace:*",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
