{
  "name": "reynard-dashboard",
  "version": "0.2.0",
  "description": "Reynard Dashboard - Integration package combining charts and components for comprehensive dashboards",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist/**/*", "README.md"],
  "scripts": {
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=dashboard-package ../../scripts/vitest-global-queue.sh run dashboard-package",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": ["solidjs", "dashboard", "charts", "components", "visualization", "analytics"],
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "reynard-charts": "workspace:*",
    "reynard-components": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-colors": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-fluent-icons": "workspace:*"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.4.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
