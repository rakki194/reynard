{
  "name": "reynard-composables",
  "version": "0.1.2",
  "description": "Reusable reactive logic for Reynard applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=composables-package ../../scripts/testing/vitest-global-queue.sh run composables-package",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["composables", "solidjs", "reactive", "hooks", "reynard"],
  "author": "Balazs Horvath",
  "license": "MIT",
  "dependencies": {
    "reynard-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
