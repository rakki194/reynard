{
  "name": "reynard-unified-repository",
  "version": "0.1.0",
  "description": "Unified repository and multimodal dataset management system for Reynard",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=unified-repository-package ../../scripts/vitest-global-queue.sh run unified-repository-package",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "test:docs": "vitest run --config vitest.docs.config.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "reynard-file-processing": "workspace:*",
    "reynard-rag": "workspace:*",
    "reynard-multimodal": "workspace:*",
    "reynard-ai-shared": "workspace:*",
    "reynard-service-manager": "workspace:*",
    "reynard-connection": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-components": "workspace:*",
    "parquetjs": "^0.11.2"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "keywords": ["reynard", "repository", "multimodal", "dataset", "parquet", "search", "rag"],
  "author": "Reynard Team",
  "license": "MIT"
}
