{
    "name": "@reynard/boundingbox",
    "version": "0.1.0",
    "description": "Reusable bounding box and annotation editing components for Reynard",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./styles": "./dist/styles.css"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && vite build",
        "dev": "vite build --watch",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest --watch",
        "type-check": "tsc --noEmit",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix"
    },
    "keywords": [
        "reynard",
        "bounding-box",
        "annotation",
        "image-editing",
        "solidjs",
        "fabricjs"
    ],
    "author": "Reynard Team",
    "license": "MIT",
    "dependencies": {
        "solid-js": "1.9.9",
        "fabric": "6.7.1"
    },
    "devDependencies": {
        "@solidjs/testing-library": "0.8.10",
        "@testing-library/jest-dom": "6.8.0",
        "@types/fabric": "5.3.10",
        "@typescript-eslint/eslint-plugin": "8.42.0",
        "@typescript-eslint/parser": "8.42.0",
        "eslint": "9.35.0",
        "jsdom": "26.1.0",
        "typescript": "5.9.2",
        "vite": "7.1.4",
        "vite-plugin-solid": "2.11.8",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "solid-js": "1.9.9"
    }
}