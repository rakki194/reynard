{
  "name": "reynard-model-management",
  "version": "0.1.2",
  "description": "Model management system for Reynard - handles ML model loading, downloading, and lifecycle management",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./managers": {
      "types": "./dist/managers/index.d.ts",
      "import": "./dist/managers/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "pnpm clean && vite build && tsc -p tsconfig.build.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:watch": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "pnpm clean:all",
    "clean:all": "rm -rf dist && rm -f *.tsbuildinfo && find . -name '*.js' -not -path './node_modules/*' -not -path './dist/*' -delete && find . -name '*.d.ts' -not -path './node_modules/*' -not -path './dist/*' -delete && find . -name '*.js.map' -not -path './node_modules/*' -not -path './dist/*' -delete && find . -name '*.d.ts.map' -not -path './node_modules/*' -not -path './dist/*' -delete",
    "clean:build": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "prebuild": "pnpm clean:build"
  },
  "keywords": ["model", "management", "ml", "ai", "download", "lifecycle"],
  "dependencies": {
    "reynard-core": "workspace:*",
    "reynard-service-manager": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  }
}
