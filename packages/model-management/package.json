{
    "name": "@reynard/model-management",
    "version": "0.1.0",
    "description": "Model management system for Reynard - handles ML model loading, downloading, and lifecycle management",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./types": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/types/index.js"
        },
        "./managers": {
            "types": "./dist/managers/index.d.ts",
            "import": "./dist/managers/index.js"
        },
        "./models": {
            "types": "./dist/models/index.d.ts",
            "import": "./dist/models/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite build --watch",
        "build": "vite build && tsc -p tsconfig.json --emitDeclarationOnly",
        "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:coverage:check": "vitest run --coverage --reporter=verbose",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "model",
        "management",
        "ml",
        "ai",
        "download",
        "lifecycle"
    ],
    "dependencies": {
        "@reynard/core": "file:../core",
        "@reynard/service-manager": "file:../service-manager"
    },
    "devDependencies": {
        "@types/node": "24.3.1",
        "@vitest/coverage-v8": "3.2.4",
        "@vitest/ui": "3.2.4",
        "typescript": "5.9.2",
        "vite": "7.1.4",
        "vite-plugin-solid": "2.11.8",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "solid-js": "1.9.9"
    }
}