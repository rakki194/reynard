{
  "name": "reynard-docs-generator",
  "version": "0.1.2",
  "description": "Automated documentation generator for Reynard packages",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "bin": {
    "reynard-docs": "./dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "generate": "node dist/cli/index.js",
    "generate:watch": "node dist/cli/index.js --watch"
  },
  "keywords": [
    "documentation",
    "generator",
    "automation",
    "reynard",
    "typescript"
  ],
  "author": "Reynard Team",
  "license": "MIT",
  "dependencies": {
    "typescript": "5.9.2",
    "glob": "11.0.3",
    "chokidar": "4.0.3",
    "fs-extra": "11.3.1",
    "gray-matter": "4.0.3",
    "marked": "16.2.1",
    "highlight.js": "11.11.1",
    "reynard-docs-core": "workspace:*"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "typescript": "5.9.2",
    "vite": "7.1.4",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
