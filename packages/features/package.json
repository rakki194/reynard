{
  "name": "reynard-features",
  "version": "0.1.2",
  "description": "Advanced feature system for managing application features, dependencies, and capabilities",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "import": "./dist/registry.js",
      "require": "./dist/registry.cjs"
    },
    "./dependencies": {
      "types": "./dist/dependencies.d.ts",
      "import": "./dist/dependencies.js",
      "require": "./dist/dependencies.cjs"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "import": "./dist/presets.js",
      "require": "./dist/presets.cjs"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc --build",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "keywords": [
    "reynard",
    "features",
    "feature-system",
    "dependencies",
    "capabilities",
    "solidjs",
    "monorepo"
  ],
  "author": "Kade",
  "license": "MIT",
  "dependencies": {
    "reynard-composables": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "eslint": "9.35.0",
    "eslint-plugin-solid": "0.14.5",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
