{
  "name": "reynard-git-automation",
  "version": "0.1.0",
  "description": "Modular Git workflow automation tools for the Reynard ecosystem",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./junk-detector": {
      "import": "./dist/junk-detector.js",
      "types": "./dist/junk-detector.d.ts"
    },
    "./change-analyzer": {
      "import": "./dist/change-analyzer.js",
      "types": "./dist/change-analyzer.d.ts"
    },
    "./commit-generator": {
      "import": "./dist/commit-generator.js",
      "types": "./dist/commit-generator.d.ts"
    },
    "./changelog-manager": {
      "import": "./dist/changelog-manager.js",
      "types": "./dist/changelog-manager.d.ts"
    },
    "./version-manager": {
      "import": "./dist/version-manager.js",
      "types": "./dist/version-manager.d.ts"
    },
    "./workflow-orchestrator": {
      "import": "./dist/workflow-orchestrator.js",
      "types": "./dist/workflow-orchestrator.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["git", "automation", "workflow", "reynard", "changelog", "versioning"],
  "author": "Reynard Team",
  "license": "MIT",
  "dependencies": {
    "commander": "14.0.1",
    "semver": "7.7.2",
    "chalk": "5.6.2",
    "ora": "9.0.0",
    "inquirer": "12.9.6",
    "execa": "9.6.0",
    "glob": "11.0.3",
    "fast-glob": "3.3.3"
  },
  "devDependencies": {
    "@types/node": "24.5.1",
    "@types/semver": "7.7.1",
    "@types/inquirer": "9.0.9",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "eslint": "9.35.0",
    "prettier": "3.6.2"
  },
  "files": ["dist", "README.md"]
}
