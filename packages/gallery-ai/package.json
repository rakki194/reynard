{
  "name": "reynard-gallery-ai",
  "version": "0.2.0",
  "description": "AI-enhanced gallery components for Reynard - integrates caption generation, batch annotation, and smart features with the gallery system",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./src/styles/ai-gallery.css": "./src/styles/ai-gallery.css"
  },
  "files": [
    "dist/**/*",
    "src/styles/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "test": "source ../../vitest.env.global && VITEST_AGENT_ID=gallery-ai-package ../../scripts/vitest-global-queue.sh run gallery-ai-package",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solidjs",
    "gallery",
    "ai",
    "caption-generation",
    "annotation",
    "batch-processing",
    "image-analysis",
    "smart-features"
  ],
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "reynard-gallery": "workspace:*",
    "reynard-annotating": "workspace:*",
    "reynard-annotating-core": "workspace:*",
    "reynard-caption": "workspace:*",
    "reynard-ai-shared": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-components": "workspace:*",
    "@solid-primitives/intersection-observer": "2.2.2",
    "@solid-primitives/resize-observer": "2.1.3",
    "@solid-primitives/event-listener": "2.4.3"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.4.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "^18.0.1",
    "reynard-testing": "workspace:*",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
