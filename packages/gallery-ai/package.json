{
  "name": "reynard-gallery-ai",
  "version": "0.1.2",
  "description": "AI-enhanced gallery components for Reynard - integrates caption generation, batch annotation, and smart features with the gallery system",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solidjs",
    "gallery",
    "ai",
    "caption-generation",
    "annotation",
    "batch-processing",
    "image-analysis",
    "smart-features"
  ],
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "dependencies": {
    "reynard-gallery": "workspace:*",
    "reynard-annotating": "workspace:*",
    "reynard-caption": "workspace:*",
    "reynard-ai-shared": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-components": "workspace:*",
    "@solid-primitives/intersection-observer": "2.2.2",
    "@solid-primitives/resize-observer": "2.1.3",
    "@solid-primitives/event-listener": "2.4.3"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@testing-library/jest-dom": "6.8.0",
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "jsdom": "26.1.0",
    "solid-js": "1.9.9",
    "typescript": "5.9.2",
    "vite": "7.1.4",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
