{
  "name": "reynard-starter-template",
  "version": "0.2.0",
  "description": "Reynard Starter Template - A SolidJS application template",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,json}'"
  },
  "dependencies": {
    "reynard-api-client": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-charts": "workspace:*",
    "reynard-components": "workspace:*",
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@types/node": "24.3.1",
    "@types/three": "0.180.0",
    "@vitest/coverage-v8": "3.2.4",
    "happy-dom": "^18.0.1",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  }
}
