{
  "name": "@reynard/starter-template",
  "version": "0.1.0",
  "description": "Reynard Starter Template - A SolidJS application template",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,json}'"
  },
  "dependencies": {
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.4",
    "@types/node": "^20.11.17",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-solid": "^2.11.8",
    "vitest": "^3.2.4"
  }
}
