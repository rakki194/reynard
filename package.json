{
  "name": "reynard",
  "version": "0.24.0",
  "private": false,
  "type": "module",
  "description": "Reynard - A cunning SolidJS framework and UI library",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakki194/reynard"
  },
  "keywords": [
    "solidjs",
    "ui",
    "framework",
    "template",
    "components",
    "themes",
    "i18n",
    "internationalization",
    "translations"
  ],
  "author": "Balazs Horvath",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter templates/starter dev",
    "dev:backend": "cd backend && source venv/bin/activate && python main.py",
    "dev:frontend": "pnpm --filter templates/starter dev",
    "dev:both": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:status": "dev-server status",
    "dev:check": "dev-server status",
    "dev:stop-all": "dev-server stop-all",
    "dev:list": "dev-server list",
    "api:generate": "pnpm --filter reynard-api-client generate",
    "api:watch": "pnpm --filter reynard-api-client generate:watch",
    "dev:full": "concurrently \"pnpm run dev:backend\" \"pnpm run api:watch\" \"pnpm run dev:frontend\"",
    "prebuild": "pnpm vitest:backup && pnpm vitest:regenerate",
    "build": "pnpm -r build",
    "build:all": "pnpm -r build",
    "build:backend": "cd backend && source venv/bin/activate && python -m py_compile main.py",
    "test": "source vitest.env.global && ./scripts/testing/vitest-global-queue.sh run main-workspace --run",
    "test:global": "source vitest.env.global && ./scripts/testing/vitest-global-queue.sh run main-agent --run",
    "test:global:watch": "source vitest.env.global && ./scripts/testing/vitest-global-queue.sh run main-agent",
    "test:global:coverage": "source vitest.env.global && ./scripts/testing/vitest-global-queue.sh run main-agent --coverage",
    "test:global:status": "./scripts/testing/vitest-global-queue.sh status",
    "test:global:cleanup": "./scripts/testing/vitest-global-queue.sh cleanup",
    "vitest:generate": "cd packages/dev-tools/vitest-config-generator && node dist/cli.js --verbose --output ../../vitest.generated.config.ts",
    "vitest:regenerate": "pnpm vitest:generate && mv vitest.generated.config.ts vitest.global.config.ts",
    "vitest:backup": "cp vitest.global.config.ts vitest.global.config.ts.backup.$(date +%Y%m%d_%H%M%S) 2>/dev/null || true",
    "test:security": "pnpm --filter reynard-core test -- --run src/security/",
    "test:backend": "cd backend && source venv/bin/activate && python -m pytest",
    "test:coverage": "pnpm -r test:coverage",
    "test:coverage:check": "pnpm -r test:coverage:check",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,css,json,md}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,css,json,md}'",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "setup": "pnpm install && pnpm run build",
    "setup:dev": "chmod +x scripts/dev-setup.sh && ./scripts/dev-setup.sh",
    "setup:nginx": "chmod +x scripts/setup-nginx-arch.sh && ./scripts/setup-nginx-arch.sh",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm run build && changeset publish",
    "audit": "pnpm audit --audit-level=moderate",
    "audit:fix": "pnpm audit --fix",
    "security:check": "pnpm audit --audit-level=high && pnpm run security:scan",
    "security:scan": "pnpm dlx audit-ci --config audit-ci.json",
    "doc-tests": "node scripts/doc-tests.js",
    "doc-tests:generate": "node scripts/doc-tests.js generate",
    "doc-tests:validate": "node scripts/doc-tests.js validate",
    "doc-tests:test": "node scripts/doc-tests.js test",
    "doc-tests:demo": "node scripts/demo-doc-tests.js",
    "doc-tests:validate-simple": "node scripts/simple-doc-validation.js",
    "docs:generate": "pnpm --filter reynard-docs-generator generate",
    "docs:watch": "pnpm --filter reynard-docs-generator generate:watch",
    "docs:dev": "pnpm --filter reynard-docs-site dev",
    "docs:build": "pnpm --filter reynard-docs-site build",
    "docs:serve": "pnpm --filter reynard-docs-site serve",
    "python:setup": "chmod +x scripts/setup-python-dev.sh && ./scripts/setup-python-dev.sh",
    "python:format": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && black . && isort .; else echo 'Virtual environment not found, using system Python...' && python3 -m black . && python3 -m isort .; fi\"",
    "python:format:check": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && black --check . && isort --check-only .; else echo 'Virtual environment not found, using system Python...' && python3 -m black --check . && python3 -m isort --check-only .; fi\"",
    "python:lint": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && flake8 .; else echo 'Virtual environment not found, using system Python...' && flake8 .; fi\"",
    "python:typecheck": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && mypy .; else echo 'Virtual environment not found, using system Python...' && python3 -m mypy .; fi\"",
    "python:security": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && bandit -c bandit.yaml -r . -f json; else echo 'Virtual environment not found, using system Python...' && python3 -m bandit -c bandit.yaml -r . -f json; fi\"",
    "python:check": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && black --check . && isort --check-only . && flake8 . && mypy .; else echo 'Virtual environment not found, using system Python...' && python3 -m black --check . && python3 -m isort --check-only . && flake8 . && python3 -m mypy .; fi\"",
    "python:linecheck": "python3 scripts/validate_python.py",
    "python:validate": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && black --check . && isort --check-only . && flake8 . && mypy . && python3 scripts/validate_python.py; else echo 'Virtual environment not found, using system Python...' && python3 -m black --check . && python3 -m isort --check-only . && flake8 . && python3 -m mypy . && python3 scripts/validate_python.py; fi\"",
    "python:test": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && pytest; else echo 'Virtual environment not found, using system Python...' && python3 -m pytest; fi\"",
    "python:test:coverage": "bash -c \"if [ -f ~/venv/bin/activate ]; then source ~/venv/bin/activate && pytest --cov=. --cov-report=html --cov-report=term-missing; else echo 'Virtual environment not found, using system Python...' && python3 -m pytest --cov=. --cov-report=html --cov-report=term-missing; fi\"",
    "i18n:test": "./scripts/i18n-test-all.sh",
    "i18n:setup": "cd packages/core/testing && pnpm i18n:setup",
    "i18n:validate": "cd packages/core/testing && pnpm i18n:validate",
    "i18n:list": "cd packages/core/testing && pnpm i18n:list",
    "i18n:eslint": "cd packages/core/testing && pnpm i18n:eslint",
    "i18n:ci": "cd packages/core/testing && pnpm i18n:ci",
    "markdown:lint": "markdownlint '**/*.md' --ignore node_modules --ignore third_party",
    "markdown:lint:fix": "markdownlint '**/*.md' --ignore node_modules --ignore third_party --fix",
    "markdown:validate": "pnpm run markdown:toc:check && pnpm run markdown:lint && pnpm run markdown:sentence:check",
    "markdown:validate:all": "pnpm run markdown:toc:check && pnpm run markdown:lint && pnpm run markdown:sentence:check && pnpm run markdown:links:check:all && pnpm run markdown:italic:check",
    "workflow:lint": "actionlint .github/workflows/*.yml",
    "workflow:check": "pnpm run workflow:validate",
    "shell:lint": "find . -type f \\( -name \"*.sh\" -o -name \"*.bash\" -o -name \"*.zsh\" \\) -not -path \"./node_modules/*\" -not -path \"./.git/*\" -not -path \"./venv/*\" -not -path \"./backend/venv/*\" -not -path \"./third_party/*\" -exec shellcheck --rcfile=.shellcheckrc {} +",
    "shell:check": "pnpm run shell:validate",
    "husky:toolkit": "node .husky/husky-toolkit-manager.js",
    "husky:toolkit:list": "node .husky/husky-toolkit-manager.js list",
    "husky:toolkit:run-all": "node .husky/husky-toolkit-manager.js run-all",
    "husky:toolkit:install-deps": "node .husky/husky-toolkit-manager.js install-deps",
    "foxy:install": "node --loader ts-node/esm scripts/install-git-hooks.ts install",
    "foxy:uninstall": "node --loader ts-node/esm scripts/install-git-hooks.ts uninstall",
    "foxy:replace-husky": "pnpm foxy:install && echo 'âœ… FOXY Git hooks installed. You can now remove husky if desired.'",
    "test:catalyst": "pnpm --filter reynard-dev-tools-catalyst test",
    "test:code-quality": "pnpm --filter reynard-code-quality test",
    "test:git-hooks-installer": "vitest run scripts/__tests__/install-git-hooks.test.ts",
    "test:dev-tools": "pnpm test:catalyst && pnpm test:code-quality && pnpm test:git-hooks-installer",
    "validation:run": "pnpm --filter reynard-code-quality analyze",
    "validation:watch": "pnpm --filter reynard-code-quality analyze:watch",
    "test:vscode-linting": "pnpm --filter reynard-vscode-linting test",
    "test:coverage:vscode-linting": "pnpm --filter reynard-vscode-linting test:coverage",
    "typecheck:vscode-linting": "pnpm --filter reynard-vscode-linting typecheck",
    "build:vscode-linting": "pnpm --filter reynard-vscode-linting build",
    "test:linting": "pnpm test:code-quality && pnpm test:vscode-linting",
    "test:coverage:linting": "pnpm test:coverage:code-quality && pnpm test:coverage:vscode-linting",
    "typecheck:linting": "pnpm typecheck:code-quality && pnpm typecheck:vscode-linting",
    "build:linting": "pnpm build:code-quality && pnpm build:vscode-linting",
    "build:project-root-utils": "pnpm exec tsc scripts/utils/project-root.ts --target es2020 --module esnext --moduleResolution node --outDir scripts/utils && pnpm exec tsc scripts/utils/test-project-root.ts --target es2020 --module esnext --moduleResolution node --outDir scripts/utils && chmod +x scripts/utils/project-root.js scripts/utils/test-project-root.js",
    "package:vscode-linting": "cd packages/dev-tools/vscode-linting && npx vsce package --no-dependencies",
    "package:vscode-linting:pre-release": "cd packages/dev-tools/vscode-linting && npx vsce package --no-dependencies --pre-release",
    "publish:vscode-linting": "pnpm --filter reynard-vscode-linting publish",
    "publish:vscode-linting:pre-release": "pnpm --filter reynard-vscode-linting publish:pre-release"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@commitlint/cli": "20.0.0",
    "@commitlint/config-conventional": "20.0.0",
    "@playwright/test": "1.55.0",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "@vitest/coverage-v8": "3.2.4",
    "audit-ci": "7.1.0",
    "concurrently": "9.2.1",
    "eslint": "9.35.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-solid": "0.14.5",
    "lint-staged": "16.2.1",
    "markdownlint-cli": "0.45.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite-plugin-solid": "2.11.8",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "dev-server-management": "workspace:*",
    "mermaid": "11.12.0",
    "reynard-3d": "workspace:*",
    "reynard-algorithms": "workspace:*",
    "reynard-annotating-core": "workspace:*",
    "reynard-api-client": "workspace:*",
    "reynard-auth": "workspace:*",
    "reynard-auth-composables": "workspace:*",
    "reynard-auth-core": "workspace:*",
    "reynard-boundingbox": "workspace:*",
    "reynard-caption": "workspace:*",
    "reynard-caption-core": "workspace:*",
    "reynard-caption-multimodal": "workspace:*",
    "reynard-caption-ui": "workspace:*",
    "reynard-charts": "workspace:*",
    "reynard-chat": "workspace:*",
    "reynard-colors": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-core": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-components-utils": "workspace:*",
    "reynard-composables": "workspace:*",
    "reynard-connection": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-docs-components": "workspace:*",
    "reynard-docs-core": "workspace:*",
    "reynard-docs-generator": "workspace:*",
    "reynard-error-boundaries": "workspace:*",
    "reynard-features": "workspace:*",
    "reynard-file-processing": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-gallery": "workspace:*",
    "reynard-games": "workspace:*",
    "reynard-http-client": "workspace:*",
    "reynard-i18n": "workspace:*",
    "reynard-model-management": "workspace:*",
    "reynard-monaco": "workspace:*",
    "reynard-queue-watcher": "workspace:*",
    "reynard-rag": "workspace:*",
    "reynard-repository-core": "workspace:*",
    "reynard-repository-multimodal": "workspace:*",
    "reynard-repository-search": "workspace:*",
    "reynard-repository-storage": "workspace:*",
    "reynard-service-manager": "workspace:*",
    "reynard-settings": "workspace:*",
    "reynard-testing": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-tool-calling": "workspace:*",
    "reynard-ui": "workspace:*",
    "reynard-unified-repository": "workspace:*",
    "solid-js": "1.9.9"
  }
}
