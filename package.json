{
  "name": "reynard",
  "version": "0.7.0",
  "private": false,
  "type": "module",
  "description": "Reynard - A cunning SolidJS framework and UI library",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakki194/reynard"
  },
  "keywords": [
    "solidjs",
    "ui",
    "framework",
    "template",
    "components",
    "themes",
    "i18n",
    "internationalization",
    "translations"
  ],
  "author": "Balazs Horvath",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter templates/starter dev",
    "dev:backend": "cd backend && source venv/bin/activate && python main.py",
    "dev:frontend": "pnpm --filter templates/starter dev",
    "dev:both": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:status": "bash .husky/dev-server-queue.sh status",
    "dev:check": "bash .husky/dev-server-queue.sh check .",
    "dev:stop-all": "bash scripts/dev-server-manager.sh stop-all",
    "dev:list": "bash scripts/dev-server-manager.sh list",
    "api:generate": "pnpm --filter reynard-api-client generate",
    "api:watch": "pnpm --filter reynard-api-client generate:watch",
    "dev:full": "concurrently \"pnpm run dev:backend\" \"pnpm run api:watch\" \"pnpm run dev:frontend\"",
    "build": "pnpm -r build",
    "build:all": "pnpm -r build",
    "build:backend": "cd backend && source venv/bin/activate && python -m py_compile main.py",
    "test": "source vitest.env.global && ./scripts/vitest-global-queue.sh run main-workspace --run",
    "test:global": "source vitest.env.global && ./scripts/vitest-global-queue.sh run main-agent --run",
    "test:global:watch": "source vitest.env.global && ./scripts/vitest-global-queue.sh run main-agent",
    "test:global:coverage": "source vitest.env.global && ./scripts/vitest-global-queue.sh run main-agent --coverage",
    "test:global:status": "./scripts/vitest-global-queue.sh status",
    "test:global:cleanup": "./scripts/vitest-global-queue.sh cleanup",
    "test:security": "pnpm --filter reynard-core test -- --run src/security/",
    "test:backend": "cd backend && source venv/bin/activate && python -m pytest",
    "test:coverage": "pnpm -r test:coverage",
    "test:coverage:check": "pnpm -r test:coverage:check",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,css,json,md}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,css,json,md}'",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "setup": "pnpm install && pnpm run build",
    "setup:dev": "chmod +x scripts/dev-setup.sh && ./scripts/dev-setup.sh",
    "setup:nginx": "chmod +x scripts/setup-nginx-arch.sh && ./scripts/setup-nginx-arch.sh",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm run build && changeset publish",
    "audit": "pnpm audit --audit-level=moderate",
    "audit:fix": "pnpm audit --fix",
    "security:check": "pnpm audit --audit-level=high && pnpm run security:scan",
    "security:scan": "pnpm dlx audit-ci --config audit-ci.json",
    "doc-tests": "node scripts/doc-tests.js",
    "doc-tests:generate": "node scripts/doc-tests.js generate",
    "doc-tests:validate": "node scripts/doc-tests.js validate",
    "doc-tests:test": "node scripts/doc-tests.js test",
    "doc-tests:demo": "node scripts/demo-doc-tests.js",
    "doc-tests:validate-simple": "node scripts/simple-doc-validation.js",
    "docs:generate": "pnpm --filter reynard-docs-generator generate",
    "docs:watch": "pnpm --filter reynard-docs-generator generate:watch",
    "docs:dev": "pnpm --filter reynard-docs-site dev",
    "docs:build": "pnpm --filter reynard-docs-site build",
    "docs:serve": "pnpm --filter reynard-docs-site serve",
    "python:setup": "chmod +x scripts/setup-python-dev.sh && ./scripts/setup-python-dev.sh",
    "python:format": "bash -c \"source ~/venv/bin/activate && black . && isort .\"",
    "python:format:check": "bash -c \"source ~/venv/bin/activate && black --check . && isort --check-only .\"",
    "python:lint": "bash -c \"source ~/venv/bin/activate && flake8 .\"",
    "python:typecheck": "bash -c \"source ~/venv/bin/activate && mypy .\"",
    "python:security": "bash -c \"source ~/venv/bin/activate && bandit -r . -x ./node_modules,./venv,./backend/venv,./third_party,./.git,./__pycache__,./backend/__pycache__,./scripts/__pycache__,./fenrir/__pycache__,./libraries/__pycache__\"",
    "python:check": "bash -c \"source ~/venv/bin/activate && black --check . && isort --check-only . && flake8 . && mypy .\"",
    "python:linecheck": "python3 scripts/validate_python.py",
    "python:validate": "bash -c \"source ~/venv/bin/activate && black --check . && isort --check-only . && flake8 . && mypy . && python3 scripts/validate_python.py\"",
    "python:test": "bash -c \"source ~/venv/bin/activate && pytest\"",
    "python:test:coverage": "bash -c \"source ~/venv/bin/activate && pytest --cov=. --cov-report=html --cov-report=term-missing\"",
    "i18n:test": "./scripts/i18n-test-all.sh",
    "i18n:setup": "cd packages/testing && pnpm i18n:setup",
    "i18n:validate": "cd packages/testing && pnpm i18n:validate",
    "i18n:list": "cd packages/testing && pnpm i18n:list",
    "i18n:eslint": "cd packages/testing && pnpm i18n:eslint",
    "i18n:ci": "cd packages/testing && pnpm i18n:ci",
    "markdown:toc:check": "node scripts/validation/markdown/validate-markdown-toc.js",
    "markdown:toc:fix": "node scripts/validation/markdown/validate-markdown-toc.js --fix",
    "markdown:lint": "markdownlint '**/*.md' --ignore node_modules --ignore third_party",
    "markdown:lint:fix": "markdownlint '**/*.md' --ignore node_modules --ignore third_party --fix",
    "markdown:sentence:check": "node scripts/validation/markdown/validate-sentence-length.js",
    "markdown:sentence:fix": "node scripts/validation/markdown/validate-sentence-length.js --fix",
    "markdown:sentence:test": "vitest run scripts/validation/markdown/validate-sentence-length.test.js",
    "markdown:validate": "pnpm run markdown:toc:check && pnpm run markdown:lint && pnpm run markdown:sentence:check",
    "markdown:links:check": "node scripts/validation/markdown/validate-markdown-links.js --staged",
    "markdown:links:check:all": "node scripts/validation/markdown/validate-markdown-links.js --all",
    "markdown:italic:check": "node scripts/validation/markdown/validate-italic-to-blockquote.js",
    "markdown:italic:fix": "node scripts/validation/markdown/validate-italic-to-blockquote.js --fix",
    "markdown:italic:fix:all": "node scripts/validation/markdown/validate-italic-to-blockquote.js --fix --all",
    "markdown:italic:test": "vitest run scripts/validation/markdown/validate-italic-to-blockquote.test.js",
    "markdown:validate:all": "pnpm run markdown:toc:check && pnpm run markdown:lint && pnpm run markdown:sentence:check && pnpm run markdown:links:check:all && pnpm run markdown:italic:check",
    "workflow:validate": "./scripts/validate-workflows.sh",
    "workflow:lint": "actionlint .github/workflows/*.yml",
    "workflow:check": "pnpm run workflow:validate",
    "shell:validate": "./scripts/validate-shell-scripts.sh",
    "shell:lint": "find . -type f \\( -name \"*.sh\" -o -name \"*.bash\" -o -name \"*.zsh\" \\) -not -path \"./node_modules/*\" -not -path \"./.git/*\" -not -path \"./venv/*\" -not -path \"./backend/venv/*\" -not -path \"./third_party/*\" -exec shellcheck --rcfile=.shellcheckrc {} +",
    "shell:check": "pnpm run shell:validate",
    "workflow:shell:extract": "node .husky/extract-workflow-shell.js",
    "workflow:shell:extract:fix": "node .husky/extract-workflow-shell.js --fix",
    "workflow:shell:validate": "node .husky/extract-workflow-shell.js --verbose",
    "husky:toolkit": "node .husky/husky-toolkit-manager.js",
    "husky:toolkit:list": "node .husky/husky-toolkit-manager.js list",
    "husky:toolkit:run-all": "node .husky/husky-toolkit-manager.js run-all",
    "husky:toolkit:install-deps": "node .husky/husky-toolkit-manager.js install-deps"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@playwright/test": "1.55.0",
    "@types/node": "24.4.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "audit-ci": "7.1.0",
    "concurrently": "9.2.1",
    "eslint": "8.57.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-solid": "0.14.5",
    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "markdownlint-cli": "0.45.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "reynard-3d": "workspace:*",
    "reynard-algorithms": "workspace:*",
    "reynard-annotating": "workspace:*",
    "reynard-api-client": "workspace:*",
    "reynard-auth": "workspace:*",
    "reynard-boundingbox": "workspace:*",
    "reynard-caption": "workspace:*",
    "reynard-caption-core": "workspace:*",
    "reynard-caption-multimodal": "workspace:*",
    "reynard-caption-ui": "workspace:*",
    "reynard-charts": "workspace:*",
    "reynard-chat": "workspace:*",
    "reynard-colors": "workspace:*",
    "reynard-components": "workspace:*",
    "reynard-components-charts": "workspace:*",
    "reynard-components-core": "workspace:*",
    "reynard-components-dashboard": "workspace:*",
    "reynard-components-themes": "workspace:*",
    "reynard-composables": "workspace:*",
    "reynard-connection": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-docs-components": "workspace:*",
    "reynard-docs-core": "workspace:*",
    "reynard-docs-generator": "workspace:*",
    "reynard-error-boundaries": "workspace:*",
    "reynard-features": "workspace:*",
    "reynard-file-processing": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-gallery": "workspace:*",
    "reynard-games": "workspace:*",
    "reynard-i18n": "workspace:*",
    "reynard-model-management": "workspace:*",
    "reynard-monaco": "workspace:*",
    "reynard-queue-watcher": "workspace:*",
    "reynard-rag": "workspace:*",
    "reynard-repository-core": "workspace:*",
    "reynard-repository-multimodal": "workspace:*",
    "reynard-repository-search": "workspace:*",
    "reynard-repository-storage": "workspace:*",
    "reynard-service-manager": "workspace:*",
    "reynard-settings": "workspace:*",
    "reynard-testing": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-tools": "workspace:*",
    "reynard-ui": "workspace:*",
    "reynard-unified-repository": "workspace:*",
    "solid-js": "1.9.9"
  }
}
