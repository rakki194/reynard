{
  "name": "reynard",
  "version": "0.2.0",
  "private": false,
  "type": "module",
  "description": "Reynard - A cunning SolidJS framework and UI library",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakki194/reynard"
  },
  "keywords": [
    "solidjs",
    "ui",
    "framework",
    "template",
    "components",
    "themes",
    "i18n",
    "internationalization",
    "translations"
  ],
  "author": "Balazs Horvath",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter templates/starter dev",
    "dev:backend": "cd backend && source venv/bin/activate && python main.py",
    "dev:frontend": "pnpm --filter templates/starter dev",
    "dev:both": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "api:generate": "pnpm --filter reynard-api-client generate",
    "api:watch": "pnpm --filter reynard-api-client generate:watch",
    "dev:full": "concurrently \"pnpm run dev:backend\" \"pnpm run api:watch\" \"pnpm run dev:frontend\"",
    "build": "pnpm -r build",
    "build:all": "pnpm -r build",
    "build:backend": "cd backend && source venv/bin/activate && python -m py_compile main.py",
    "test": "pnpm -r test",
    "test:security": "pnpm --filter reynard-core test -- --run src/security/",
    "test:backend": "cd backend && source venv/bin/activate && python -m pytest",
    "test:coverage": "pnpm -r test:coverage",
    "test:coverage:check": "pnpm -r test:coverage:check",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,css,json,md}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,css,json,md}'",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "setup": "pnpm install && pnpm run build",
    "setup:dev": "chmod +x scripts/dev-setup.sh && ./scripts/dev-setup.sh",
    "setup:nginx": "chmod +x scripts/setup-nginx-arch.sh && ./scripts/setup-nginx-arch.sh",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm run build && changeset publish",
    "audit": "pnpm audit --audit-level=moderate",
    "audit:fix": "pnpm audit --fix",
    "security:check": "pnpm audit --audit-level=high && pnpm run security:scan",
    "security:scan": "pnpm dlx audit-ci --config audit-ci.json",
    "doc-tests": "node scripts/doc-tests.js",
    "doc-tests:generate": "node scripts/doc-tests.js generate",
    "doc-tests:validate": "node scripts/doc-tests.js validate",
    "doc-tests:test": "node scripts/doc-tests.js test",
    "doc-tests:demo": "node scripts/demo-doc-tests.js",
    "doc-tests:validate-simple": "node scripts/simple-doc-validation.js",
    "docs:generate": "pnpm --filter reynard-docs-generator generate",
    "docs:watch": "pnpm --filter reynard-docs-generator generate:watch",
    "docs:dev": "pnpm --filter reynard-docs-site dev",
    "docs:build": "pnpm --filter reynard-docs-site build",
    "docs:serve": "pnpm --filter reynard-docs-site serve",
    "python:setup": "chmod +x scripts/setup-python-dev.sh && ./scripts/setup-python-dev.sh",
    "python:format": "bash -c \"source ~/venv/bin/activate && black . && isort .\"",
    "python:format:check": "bash -c \"source ~/venv/bin/activate && black --check . && isort --check-only .\"",
    "python:lint": "bash -c \"source ~/venv/bin/activate && flake8 .\"",
    "python:typecheck": "bash -c \"source ~/venv/bin/activate && mypy .\"",
    "python:security": "bash -c \"source ~/venv/bin/activate && bandit -r .\"",
    "python:check": "bash -c \"source ~/venv/bin/activate && black --check . && isort --check-only . && flake8 . && mypy .\"",
    "python:linecheck": "python3 .husky/validate-python.py",
    "python:validate": "bash -c \"source ~/venv/bin/activate && black --check . && isort --check-only . && flake8 . && mypy . && python3 .husky/validate-python.py\"",
    "python:test": "bash -c \"source ~/venv/bin/activate && pytest\"",
    "python:test:coverage": "bash -c \"source ~/venv/bin/activate && pytest --cov=. --cov-report=html --cov-report=term-missing\"",
    "i18n:test": "./scripts/i18n-test-all.sh",
    "i18n:setup": "cd packages/testing && pnpm i18n:setup",
    "i18n:validate": "cd packages/testing && pnpm i18n:validate",
    "i18n:list": "cd packages/testing && pnpm i18n:list",
    "i18n:eslint": "cd packages/testing && pnpm i18n:eslint",
    "i18n:ci": "cd packages/testing && pnpm i18n:ci",
    "markdown:toc:check": "node .husky/validate-markdown-toc.js",
    "markdown:toc:fix": "node .husky/validate-markdown-toc.js --fix",
    "markdown:lint": "markdownlint '**/*.md' --ignore node_modules --ignore third_party",
    "markdown:lint:fix": "markdownlint '**/*.md' --ignore node_modules --ignore third_party --fix",
    "markdown:validate": "pnpm run markdown:toc:check && pnpm run markdown:lint"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.6",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/node": "24.3.1",
    "@typescript-eslint/eslint-plugin": "8.42.0",
    "@typescript-eslint/parser": "8.42.0",
    "audit-ci": "7.1.0",
    "concurrently": "9.2.1",
    "eslint": "9.35.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-solid": "0.14.5",
    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "markdownlint-cli": "^0.45.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "reynard-3d": "workspace:*",
    "reynard-algorithms": "workspace:*",
    "reynard-annotating": "workspace:*",
    "reynard-api-client": "workspace:*",
    "reynard-auth": "workspace:*",
    "reynard-boundingbox": "workspace:*",
    "reynard-caption": "workspace:*",
    "reynard-charts": "workspace:*",
    "reynard-chat": "workspace:*",
    "reynard-colors": "workspace:*",
    "reynard-components": "workspace:*",
    "reynard-composables": "workspace:*",
    "reynard-connection": "workspace:*",
    "reynard-core": "workspace:*",
    "reynard-docs-components": "workspace:*",
    "reynard-docs-core": "workspace:*",
    "reynard-docs-generator": "workspace:*",
    "reynard-error-boundaries": "workspace:*",
    "reynard-features": "workspace:*",
    "reynard-file-processing": "workspace:*",
    "reynard-fluent-icons": "workspace:*",
    "reynard-gallery": "workspace:*",
    "reynard-games": "workspace:*",
    "reynard-i18n": "workspace:*",
    "reynard-model-management": "workspace:*",
    "reynard-monaco": "workspace:*",
    "reynard-rag": "workspace:*",
    "reynard-service-manager": "workspace:*",
    "reynard-settings": "workspace:*",
    "reynard-testing": "workspace:*",
    "reynard-themes": "workspace:*",
    "reynard-tools": "workspace:*",
    "reynard-ui": "workspace:*"
  }
}
